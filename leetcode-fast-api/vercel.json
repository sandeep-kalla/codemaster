{
  "builds": [
    {
      "src": "run.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "run.py"
    },
    {
      "src": "/docs",
      "dest": "run.py"
    },
    {
      "src": "/redoc",
      "dest": "run.py"
    },
    {
      "src": "/openapi.json",
      "dest": "run.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "app/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "run.py"
    }
  ],
  "env": {
    "APP_MODULE": "app.main:app",
    "PYTHONPATH": ".",
    "VERCEL_DEPLOYMENT": "1"
  }
}
